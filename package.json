{
  "name": "overwatch",
  "version": "2.0.0",
  "description": "FiveM Server Discord Bot developed by EngineerShawn",
  "main": "./server",
  "scripts": {
    "start": "node server/server.js",
    "prepare": "is-ci || husky install",
    "test": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EngineerShawn/overwatch.git"
  },
  "author": "EngineerPatterson",
  "contributors": [
    "EngineerPatterson(https://github.com/EngineerShawn/)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EngineerShawn/Overwatch/issues"
  },
  "homepage": "https://github.com/EngineerShawn/Overwatch",
  "dependencies": {
    "discord.js": "github:discordjs/discord.js",
    "dotenv": "^16.5.0"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 2023,
      "sourceType": "script"
    },
    "devDependencies": {
      "husky": "^8.0.0",
      "sourceType": "script"
    },
    "rules": {
      "arrow-spacing": [
        "warn",
        {
          "before": true,
          "after": true
        }
      ],
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "comma-spacing": "error",
      "comma-style": "error",
      "curly": [
        "error",
        "multi-line",
        "consistent"
      ],
      "dot-location": [
        "error",
        "property"
      ],
      "handle-callback-err": "off",
      "indent": [
        "error",
        4
      ],
      "keyword-spacing": "error",
      "max-nested-callbacks": [
        "error",
        {
          "max": 4
        }
      ],
      "max-statements-per-line": [
        "error",
        {
          "max": 2
        }
      ],
      "no-console": "off",
      "no-empty-function": "error",
      "no-floating-decimal": "error",
      "no-inline-comments": "error",
      "no-lonely-if": "error",
      "no-multi-spaces": "error",
      "no-multiple-empty-lines": [
        "error",
        {
          "max": 3,
          "maxEOF": 1,
          "maxBOF": 0
        }
      ],
      "no-shadow": [
        "error",
        {
          "allow": [
            "err",
            "resolve",
            "reject"
          ]
        }
      ],
      "no-trailing-spaces": [
        "error"
      ],
      "no-var": "error",
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "prefer-const": "error",
      "quotes": [
        "error",
        "double"
      ],
      "semi": [
        "error",
        "always"
      ],
      "space-before-blocks": "error",
      "space-before-function-parent": [
        "error",
        {
          "anonymous": "never",
          "named": "never",
          "asyncArrow": "always"
        }
      ],
      "space-in-parens": "error",
      "space-infix-ops": "error",
      "space-unary-ops": "error",
      "spaced-comment": "error",
      "yoda": "error",
      "no-undef": "off",
      "no-unused-vars": "off"
    }
  },
  "devDependencies": {
    "husky": "^8.0.0"
  }
}
